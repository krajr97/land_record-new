 <%- include('layout/header')  %>
        <div class="page-wrapper">
            <div class="content container-fluid">
               <div class="page-header">
                  <div class="row">
                     <div class="col-sm-12">
                        <h3 class="page-title">Welcome  !</h3>
                        <ul class="breadcrumb">
                           <li class="breadcrumb-item active">Dashboard</li>
                        </ul>
                     </div>
                  </div>
               </div>
               <div class="row">
                  <div class="col-xl-3 col-sm-6 col-12 d-flex">
                     <div class="card bg-one w-100">
                        <a href="/lohan_list">
                           <div class="card-body">
                              <div class="db-widgets d-flex justify-content-between align-items-center">
                                 <div class="db-icon">
                                    <i class="fas fa-user-graduate"></i>
                                 </div>
                                 <div class="db-info">
                                    <h3><%- lohan_count %></h3>
                                    <h6>Lohan</h6>
                                 </div>
                              </div>
                           </div>
                        </a>
                     </div>
                  </div>
                  <div class="col-xl-3 col-sm-6 col-12 d-flex">
                     <div class="card bg-two w-100">
                        <a href="/bhalua_list">
                           <div class="card-body">
                              <div class="db-widgets d-flex justify-content-between align-items-center">
                                 <div class="db-icon">
                                    <i class="fas fa-crown"></i>
                                 </div>
                                 <div class="db-info">
                                    <h3><%- bhalua_count %></h3>
                                    <h6>Bhalua</h6>
                                 </div>
                              </div>
                           </div>
                        </a>
                     </div>
                  </div>
                  <div class="col-xl-3 col-sm-6 col-12 d-flex">
                     <div class="card bg-three w-100">                        
                        <div class="card-body">
                           <div class="db-widgets d-flex justify-content-between align-items-center">
                              <div class="db-icon">
                                 <i class="fas fa-building"></i>
                              </div>
                              <div class="db-info">
                                 <h3><%- lutaut_count %></h3>
                                 <h6>Lutaut</h6>
                              </div>
                           </div>
                        </div>
                     </div>
                  </div>
                  <div class="col-xl-3 col-sm-6 col-12 d-flex">
                     <div class="card bg-four w-100">
                        <div class="card-body">
                           <div class="db-widgets d-flex justify-content-between align-items-center">
                              <div class="db-icon">
                                 <i class="fas fa-file-invoice-dollar"></i>
                              </div>
                              <div class="db-info">
                                 <h3><%- nathNagar_count %></h3>
                                 <h6>Nath Nagar</h6>
                              </div>
                           </div>
                        </div>
                     </div>
                  </div>
               </div>
               <div class="row">
                  <div class="col-md-6 d-flex">
                     <div class="card flex-fill">
                        <div class="card-header">
                           <h5 class="card-title">Lohan List</h5>
                        </div>
                        <div class="card-body">
                           <div class="table-responsive">
                              <table class="table table-bordered  table-hover table-center" id="lohan_tableList_id" style="width: 100%;">
                                 <thead class="thead-light">
                                    <tr>
                                       <th>Id</th>                                       
                                       <th class="text-center">Khata</th>
                                       <th class="text-center">Keshera</th>
                                       <th class="text-center">Acre</th>
                                       <th class="text-center">Dismil</th>
                                    </tr>
                                 </thead>
                                 <tbody>
                                    <%   
                                       if(lohan_list !=''){ %>
                                       <% lohan_list.forEach((row,index) =>{ %>
                                    <tr>
                                       <td class="text-center">
                                          <div><%- index+1;  %></div>
                                       </td>
                                       <td class="text-center"><%- row.khata;  %></td>
                                       <td class="text-center"><%- row.keshera;  %></td>
                                       <td class="text-center"><%- row.acre;  %></td>
                                       <td class="text-center">
                                          <div><%- row.dismil;  %></div>
                                       </td>
                                    </tr>
                                    <% }); %>
                                    <% } %>
                                 </tbody>
                              </table>
                           </div>
                        </div>
                     </div>
                  </div>
                  <div class="col-md-6 d-flex">
                     <div class="card flex-fill">
                        <div class="card-header">
                           <h5 class="card-title">Bhalua List</h5>
                        </div>
                        <div class="card-body">
                           <div class="table-responsive">
                              <table class="table table-bordered  table-hover table-center" id="bhalua_tableList_id" style="width: 100%;">
                                 <thead class="thead-light">
                                    <tr>
                                       <th>Id</th>                                       
                                       <th class="text-center">Khata</th>
                                       <th class="text-center">Keshera</th>
                                       <th class="text-center">Acre</th>
                                       <th class="text-center">Dismil</th>
                                    </tr>
                                 </thead>
                                 <tbody>
                                    <% if(bhalua_list !=''){ %>
                                       <% bhalua_list.forEach((row,index) =>{ %>
                                    <tr>
                                       <td class="text-center">
                                          <div><%- index+1;  %></div>
                                       </td>
                                       <td class="text-center"><%- row.khata;  %></td>
                                       <td class="text-center"><%- row.keshera;  %></td>
                                       <td class="text-center"><%- row.acre;  %></td>
                                       <td class="text-center">
                                          <div><%- row.dismil;  %></div>
                                       </td>
                                    </tr>
                                    <% }); %>
                                    <% } %>
                                 </tbody>
                              </table>
                              
                           </div>
                        </div>
                     </div>
                  </div>
               </div>
            </div>
        <%- include('layout/footer')  %>